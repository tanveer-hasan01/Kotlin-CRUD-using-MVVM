<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.projectkt">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:name=".Dependency_injection.BaseApplication"
        android:theme="@style/Theme.ProjectKT"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".room_ui.ListUsers"
            android:exported="true" />
        <activity
            android:name=".room_ui.AddActivity"
            android:exported="true">

        </activity>
        <activity android:name=".activity.UpdateActivity" />
        <activity android:name=".activity.ListActivity" />
        <activity android:name=".activity.Home">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.AddUserActivity" />
    </application>

</manifest>